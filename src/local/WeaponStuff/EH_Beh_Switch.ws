			{
				if ( thePlayer.GetBehaviorGraphInstanceName() != 'claw_beh_SCAAR' )
				{
					 stupidArray.Clear(); stupidArray.PushBack( 'claw_beh_SCAAR' );
				}
			}
			else if (ACS_GetFistMode() == 2 )
			{
				if ( thePlayer.GetBehaviorGraphInstanceName() != 'acs_sorc_beh' )
